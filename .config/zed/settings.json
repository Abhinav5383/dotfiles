{
	"telemetry": {
		"metrics": false
	},
	"vim_mode": false,
	"theme": "Monospace dark",
	"base_keymap": "VSCode",
	"buffer_font_family": "JetBrainsMono Nerd Font Mono",
	"buffer_font_size": 18,
	"buffer_font_weight": 400,
	"ui_font_family": "Inter",
	"ui_font_size": 18,
	"ui_font_weight": 400,
	"tab_size": 4,
	"remove_trailing_whitespace_on_save": true,
	"format_on_save": "on",
	"terminal": {
		"font_family": "JetBrainsMono Nerd Font Mono"
	},
	"formatter": {
		"language_server": {
			"name": "biome"
		}
	},
	"languages": {
		"CSS": {
			"formatter": "auto"
		}
	},
	"features": {
		"copilot": true
	},
	"assistant": {
		"default_model": {
			"provider": "copilot_chat",
			"model": "gpt-4o"
		},
		"enabled": true,
		"version": "2",
		"button": true
	},
	"chat_panel": {
		"button": false
	},
	"collaboration_panel": {
		"button": false
	},
	"code_actions_on_format": {
		"source.fixAll.biome": true,
		"source.organizeImports.biome": true
	}
}
